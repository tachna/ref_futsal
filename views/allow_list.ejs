<table
            
class="bbsListTbl"
summary="번호, 제목, 조회수, 작성일 등을 제공하는 표"
>

<thead>
  <tr>
    <th scope="col">신청 날짜</th>
    <th scope="col">예약자 성명</th>
    <th scope="col">학번</th>
    <th scope="col">학과</th>
    <th scope="col"><font color='red'>예약 날짜</font></th>
    <th scope="col"><font color='red'>예약 시간</font></th>
    <th scope="col">인원</th>
    <th scope="col">사용 목적</th>
    <th scope="col">이메일</th>
    <th scope="col">수락</th>
    <th scope="col">불가</th>

  </tr>
</thead>
<tbody>
  <% for (var i = 0; i < data.length; i++){ %>
  <tr>
    <td><%= data[i].r_date %></td>
    <td><%= data[i].name %></td>
    <td><%= data[i].stu_num %></td>
    <td><%= data[i].major %></td>
    <td><%= data[i].date %></td>
    <td><% if(data[i].level==1){%>6시~8시<%} %>
      <% if(data[i].level==2){%>8시~10시<%} %>
      <% if(data[i].level==3){%>10시~12시<%} %>
      <% if(data[i].level==4){%>12시~14시<%} %>
      <% if(data[i].level==5){%>14시~16시<%} %>
      <% if(data[i].level==6){%>16시~18시<%} %>
      <% if(data[i].level==7){%>18시~20시<%} %>
      <% if(data[i].level==8){%>20시~22시<%} %>
      <% if(data[i].level==9){%>22시~24시<%} %>

  </td>
    <td><%= data[i].members %></td>
    <td><%= data[i].reason %></td>
    <td><%= data[i].email %></td>
    <!-- <td class="tit_notice">
      <a href="/detail/<%= data[i]._id %>"
        ><%= data[i].wait %></a
      >
    </td> -->

    <td><font size=5><a href="/allow/<%= data[i].serial%>">수락</a></font></br></div></td>
    <td><font size=5><a href="/cancel/<%= data[i].serial%>">불가</a></font></br></div></td>
  </tr>
  <% }%>
</tbody>
</table>

                </div>
